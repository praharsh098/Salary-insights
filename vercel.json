{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "GOOGLE_GENAI_API_KEY": "@google_genai_api_key"
  },
  "rewrites": [
    {
      "source": "/genkit/(.*)",
      "destination": "/api/genkit/$1"
    }
  ]
}

